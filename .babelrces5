{
  "env": {
    "es5": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
                "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": "umd",
          },
        ],
        "@babel/preset-react",
      ],
      "plugins": ["@babel/plugin-syntax-dynamic-import"],
    }
  }
}
